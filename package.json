{
  "name": "payload-plugin-template",
  "version": "3.52.0",
  "homepage:": "https://payloadcms.com",
  "repository": "git@github.com:payloadcms/payload-plugin-template.git",
  "description": "Payload plugin template using v3",
  "author": {
    "name": "Sascha Seewald"
  },
  "contributors": [
    {
      "email": "dev@payloadcms.com",
      "name": "dev@payloadcms.com"
    }
  ],
  "license": "MIT",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "keywords": [
    "payload",
    "cms",
    "plugin",
    "typescript",
    "react",
    "template"
  ],
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.json && pnpm copyfiles",
    "build:all": "pnpm build && pnpm -C dev build",
    "build:app": "pnpm -C dev build",
    "clean": "rimraf {dist,*.tsbuildinfo,package}",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "dev": "concurrently -k -p \"[{name}]\" -n \"PLUGIN,APP\" -c \"cyan.bold,green.bold\" \"pnpm watch\" \"pnpm -C dev dev\"",
    "dev:docker": "concurrently -k -p \"[{name}]\" -n \"PLUGIN,DOCKER\" -c \"cyan.bold,magenta.bold\" \"pnpm watch\" \"pnpm -C dev docker:up\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepack": "pnpm prepublishOnly",
    "prepublishOnly": "pnpm clean && pnpm build && pnpm copyfiles",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TSC,ASSETS\" -c \"yellow.bold,blue.bold\" \"tsc --watch --project tsconfig.json\" \"pnpm watch:assets\"",
    "watch:assets": "nodemon --watch src --ext html,css,scss,ttf,woff,woff2,eot,svg,jpg,png --exec \"pnpm copyfiles\""
  },
  "peerDependencies": {
    "@payloadcms/ui": ">=3.0.0 <4.0.0",
    "next": ">=15.4.6",
    "payload": ">=3.0.0 <4.0.0",
    "react": ">=19.0.0 <20.0.0",
    "react-dom": ">=19.0.0 <20.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@payloadcms/eslint-config": "3.28.0",
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "concurrently": "^9.2.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.33.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.1",
    "typescript": "5.9.2"
  },
  "pnpm": {
    "overrides": {}
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "registry": "https://registry.npmjs.org/",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./components": {
        "import": "./dist/components/index.js",
        "default": "./dist/components/index.js",
        "types": "./dist/components/index.d.ts"
      }
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "default": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    }
  }
}
