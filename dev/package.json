{
  "name": "payload-plugin-template",
  "description": "A test app for the plugin based on npx create-payload-app - blank ",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"--no-deprecation\" next dev --turbopack",
    "devsafe": "rimraf .next && pnpm dev",
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation\" next build",
    "start": "cross-env NODE_OPTIONS=\"--no-deprecation\" next start",
    "payload": "payload",
    "docker:up": "docker compose -f docker-compose.yml up",
    "docker:down": "docker compose -f docker-compose.yml down",
    "docker:clean": "docker compose -f docker-compose.yml down -v",
    "generate:types": "payload generate:types",
    "generate:schema": "payload-graphql generate:schema",
    "generate:importmap": "payload generate:importmap"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "^3.52.0",
    "@payloadcms/db-sqlite": "^3.52.0",
    "@payloadcms/next": "3.52.0",
    "@payloadcms/richtext-lexical": "3.52.0",
    "@payloadcms/ui": "3.52.0",
    "next": "15.4.6",
    "payload": "3.52.0",
    "payload-plugin-template": "workspace:*",
    "react": "19.1.1",
    "react-dom": "19.1.1"
  },
  "devDependencies": {
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
    "rimraf": "^6.0.1",
    "typescript": "5.9.2"
  }
}
